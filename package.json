{
  "name": "@mol-fe/mol-fe-xpmodule-sponsored-video",
  "version": "0.0.0",
  "main": "dist/server/code.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MailOnline/mol-fe-xpmodule-sponsored-video.git"
  },
  "author": "MailOnline <fe@mailonline.co.uk>",
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rm -rf './dist'",
    "prebuild:dev": "rm -rf './dist'",
    "prebuild.client": "rm -rf './dist/client'",
    "prebuild.client:dev": "rm -rf './dist/client'",
    "prebuild.server": "rm -rf './dist/server'",
    "prebuild.server:dev": "rm -rf './dist/server'",
    "build": "webpack --config ./webpack/client.ts --config ./webpack/server.ts",
    "build:dev": "NODE_ENV=development webpack --config ./webpack/client.ts --config ./webpack/server.ts",
    "types": "tsm ./src --watch --logLevel=error",
    "precommit": "yarn run lint && yarn run test",
    "lint": "yarn lint:client && tsc --project ./tsconfig.json --noEmit && eslint './src/**/*.{js,jsx,ts,tsx}' './webpack/*'",
    "lint:client": "tsc --project ./src/client/tsconfig.json --noEmit",
    "test": "jest --coverage --passWithNoTests",
    "start:app": "NODE_ENV=development webpack serve --config ./webpack/client.ts --config ./webpack/server.ts",
    "start": "npm-run-all --parallel types start:app",
    "preversion": "yarn run lint && yarn run test",
    "postversion": "git push && git push --tags && npm publish",
    "publish-xp-module": "publishXpModule"
  },
  "xpModules": [
    {
      "name": "Sponsored Video",
      "displayName": "Sponsored Video Xp Module",
      "description": "Sponsored Video Xp Module",
      "supportedPlatforms": [
        "cc",
        "mol.web.desktop"
      ],
      "code": "./dist/server/code.js",
      "config": "./dist/server/config.js"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/mol-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test"
    }
  },
  "devDependencies": {
    "@mol-fe/mol-fe-loggers": "^6.1.1",
    "@mol-fe/mol-fe-react": "^5.19.1",
    "@mol-fe/mol-fe-registry-cli": "^1.7.0",
    "@mol-fe/mol-fe-registry-types": "^1.1.2",
    "@mol-fe/mol-fe-webpack-bundles": "^1.9.1",
    "@types/autoprefixer": "^9.7.2",
    "@types/jest": "^25.2.1",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/node": "^13.11.0",
    "@types/postcss-nested": "^4.1.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^17.0.1",
    "@types/webpack": "^4.41.26",
    "@types/webpack-dev-server": "^3.10.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "autoprefixer": "^9.7.5",
    "axios": "^0.19.2",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^0.28.11",
    "eslint": "^7.21.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.0.0",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "jest-teamcity-reporter": "^0.9.0",
    "mini-css-extract-plugin": "^1.3.9",
    "mol-conventional-changelog": "^1.4.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.3",
    "react": "^16.13.1",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typed-scss-modules": "^4.1.0",
    "typescript": "^4.1.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "react-dom": "^17.0.1"
  }
}
